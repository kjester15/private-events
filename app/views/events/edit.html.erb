<%= form_with model: @event do |f| %>
  <%= f.label :title, "Title" %><br>
  <%= f.text_field :title %><br><br>
  <%= f.label :description, "Description" %><br>
  <%= f.text_area :description %><br><br>
  <%= f.label :date, "Date" %><br>
  <%= f.date_select :date %><br><br>
  <%= f.submit "Submit" %>
<% end %>