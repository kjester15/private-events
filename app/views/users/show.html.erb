<h1>Your Events</h1>
<% @events.each do |event| %>
  <ul>  
    <li>
      <h3><%= event.title %></h3>
      <%= event.description %><br>
      <%= event.date %><br>
    </li>
  </ul><% end %>

<h1>Events You're attending</h1>
<% @attendances.each do |attendance| %>
  <ul>  
    <li>
      <h3><%= attendance.title %></h3>
      <%= attendance.description %><br>
      <%= attendance.date %><br>
    </li>
  </ul><% end %>